{
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko": {
      "id": "ahmedboukha126@gmail.com"
    }
  },
  "name": "Border Extension",
  "version": "1.0.1",
  "description": "Adds a border to the current page",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/16x16.jpg",
      "48": "assets/48x48.png",
      "128": "assets/128x128.jpg"
    }
  }
}
