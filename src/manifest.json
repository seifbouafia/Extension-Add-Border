{
  "manifest_version": 3,
  "name": "Angular LastPass Icon Injector",
  "version": "1.0",
  "description": "Inject LastPass icon into input fields",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
